<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	用这个配置文件 生产 数据库表
	和生成excle数据转成 insert 语句 和 表名字
	
		
	
	******************************
	晚上回去完成的功能是 读取excle 文件
	生成表和 insert语句
		
 -->
<tables>
	<table name="table" charset="UTF-8">
		<!--
		 你要转换sql语句要与excle列一一对应
		 方便生成sql语句
		 -->
		<column  name="id" type="int"/>
		<column  name="name" type="string"/>
		<column  name="price" type="double"/>
		<column  name="detail" type="string"/>
		<column  name="pic" type="string"/>
		<column  name="createtime" type="date"/>
	</table>
</tables>
